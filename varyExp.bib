issn = {08981221},
journal = {Comput. Math. with Appl.},
keywords = {Cauchy problem,Fractional calculus,Inverse subordinator,Poisson process,Tempered stable},
number = {6},
pages = {892--905},
publisher = {Elsevier Ltd},
title = {{Applications of inverse tempered stable subordinators}},
