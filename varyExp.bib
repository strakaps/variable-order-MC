journal = {Biophys. J.},
keywords = {Biological,Biophysics,Biophysics: methods,Buffers,Chemical,Dextrans,Dextrans: chemistry,Diffusion,Fluorescence,Fluorescence: methods,Fluorescent Dyes,Fluorescent Dyes: pharmacology,Ions,Macromolecular Substances,Models,Monte Carlo Method,Proteins,Proteins: chemistry,Spectrometry,Statistical,Temperature,Time Factors},
month = {nov},
